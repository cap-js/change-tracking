{
  "name": "@capire/incidents",
  "version": "1.0.0",
  "scripts": {
    "deploy": "cds deploy -2 hana"
  },
  "dependencies": {
    "@cap-js/change-tracking": "file:../../.",
    "@cap-js/attachments": "^2",
    "@cap-js/hana": "^2.3.4"
  },
  "cds": {
    "requires": {
      "auth": {
        "[development]": {
          "users": {
            "alice": {
              "roles": [
                "support",
                "admin"
              ]
            },
            "bob": {
              "roles": [
                "support"
              ]
            }
          }
        }
      },
      "attachments": {
        "kind": "db",
        "scan": false
      },
      "change-tracking": {
        "considerLocalizedValues": true,
        "preserveDeletes": false
      }
    }
  },
  "private": true
}
